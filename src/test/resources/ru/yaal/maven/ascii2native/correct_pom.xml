    <groupId>ru.yaal.maven.ascii2native</groupId>
    <artifactId>Test</artifactId>
    <version>1.0-SNAPSHOT</version>

    <build>
        <plugins>
            <plugin>
                <groupId>ru.yaal.maven</groupId>
                <artifactId>ascii2native-maven-plugin</artifactId>
